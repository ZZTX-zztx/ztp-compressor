{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ztp-encryptor",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-10",
  "assets": {
    "directory": "./public"
  },
  "rules": [
    {
      "type": "Asset",
      "globs": [
        "**/*.html",
        "**/*.css",
        "**/*.js",
        "**/*.json",
        "**/*.jsonc"
      ]
    }
  ]
}
